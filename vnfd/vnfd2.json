{
  "vnfd": {
    "tenant_id": "3a7a49ccfef943ebae2457feabc2931e",
    "name": "VNFD2",
    "description": "Sample",
    "service_types": [
      {
        "service_type": "vnfd"
      }
    ],
    "attributes": {
      "vnfd": {
        "tosca_definitions_version": "tosca_simple_profile_for_nfv_1_0_0",
        "description": "Demo example",
        "metadata": {
          "template_name": "sample-tosca-vnfd"
        },
        "topology_template": {
          "node_templates": {
            "VDU1": {
              "type": "tosca.nodes.nfv.VDU.Tacker",
              "capabilities": {
                "nfv_compute": {
                  "properties": {
                    "num_cpus": 1,
                    "mem_size": "1000 MB",
                    "disk_size": "10 GB"
                  }
                }
              },
              "properties": {
                "image": "ubuntu",
                "user_data": "#!/bin/sh;   echo 1 > /proc/sys/net/ipv4/ip_forward; mkdir /home/openstack/helllo"
              }
            },
            "CP2": {
              "type": "tosca.nodes.nfv.CP.Tacker",
              "properties": {
                "order": 0,
                "management": true,
                "anti_spoofing_protection": false
              },
              "requirements": [
                {
                  "virtualLink": {
                    "node": "VL2"
                  }
                },
                {
                  "virtualBinding": {
                    "node": "VDU1"
                  }
                }
              ]
            },
            "VL2": {
              "type": "tosca.nodes.nfv.VL",
              "properties": {
                "vendor": "Tacker",
                "network_name": "private"
              }
            }
          }
        }
      }
    }
  }
}
